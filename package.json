{
  "name": "pev2",
  "version": "0.24.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name pev2 ./src/components/Plan.vue && EXTRACT_CSS=true vue-cli-service build --target lib --name pev2 ./src/components/Plan.vue --dest ./dist/components && cp -R ./src/assets/scss ./dist/scss",
    "test:unit": "jest",
    "lint": "vue-cli-service lint",
    "build-app": "vue-cli-service build --dest dist-app",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "snyk protect"
  },
  "main": "./dist/pev2.common.js",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@types/dragscroll": "0.0.0",
    "@types/highlight.js": "^9.12.4",
    "@types/lodash": "^4.14.172",
    "@types/moment-duration-format": "^2.2.3",
    "bootstrap": "^4.6.1",
    "clarinet": "^0.12.3",
    "core-js": "^3.16.1",
    "highlight.js": "^10.7.2",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "popper.js": "^1.16.1",
    "splitpanes": "^2.3.8",
    "stream": "^0.0.2",
    "tippy.js": "^6.3.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-clipboard3": "^1.0.1",
    "vue-fragment": "^1.5.2",
    "vue-property-decorator": "^8.5.1",
    "vue-sorted-table": "^1.3.0",
    "vue-tippy": "^4.11.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@types/jest": "^25.1.2",
    "@vue/cli-plugin-babel": "~5.0.0-rc.2",
    "@vue/cli-plugin-typescript": "~5.0.0-rc.2",
    "@vue/cli-service": "~5.0.0-rc.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "husky": "^4.2.2",
    "jest": "^25.3.0",
    "lint-staged": "^10.0.7",
    "moment-locales-webpack-plugin": "^1.2.0",
    "prettier": "2.0.5",
    "raw-loader": "^4.0.2",
    "sass": "^1.49.7",
    "sass-loader": "^8.0.0",
    "snyk": "^1.334.0",
    "ts-jest": "^25.2.0",
    "typescript": "~4.1.5",
    "vue-jest": "^3.0.5",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.68.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged blah"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "PostgreSQL",
    "vuejs",
    "SQL",
    "plan"
  ],
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint"
    ],
    "*.vue": [
      "vue-cli-service lint"
    ],
    "*.{js,css,json,md}": [
      "prettier --write"
    ]
  },
  "snyk": true
}
